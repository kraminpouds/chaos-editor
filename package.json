{
  "name": "chaos-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write  \"src/**/*.{js,json,css,scss,md,ts,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,json,css,scss,md,ts,html}\"",
    "dev:ssr": "ng run chaos-editor:serve-ssr",
    "qa:ssr": "ng run chaos-editor:serve-ssr:qa",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr:qa": "ng build -c=qa && ng run chaos-editor:server:qa",
    "build:ssr:prod": "ng build --configuration production && ng run chaos-editor:server:production",
    "prerender:qa": "ng run chaos-editor:prerender:qa",
    "prerender:prod": "ng run chaos-editor:prerender:production",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer",
    "analyzer": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/cdk": "~12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/platform-server": "~12.2.3",
    "@angular/router": "~12.2.3",
    "@antv/g2plot": "^2.3.32",
    "@nguniversal/express-engine": "12.1.0",
    "axios": "^0.21.1",
    "axios-logger": "^2.5.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.16.4",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "ng-zorro-antd": "^12.0.1",
    "ngx-infinite-scroll": "^10.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "vconsole": "^3.9.1",
    "web-animations-js": "^2.3.2",
    "xhr2": "^0.2.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@nguniversal/builders": "^12.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jasmine": "^3.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "date-fns": "^2.23.0",
    "dragula": "^3.7.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jasmine-core": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng2-dragula": "^2.1.1",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.2.1",
    "typescript": "~4.3.4",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
